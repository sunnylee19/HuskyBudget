<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.slee_hdworak.huskybudget">

    <uses-feature
        android:name="android.hardware.sensor.accelerometer"
        android:required="true" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.slee_hdworak.huskybudget.ui.login.LoginActivity"
            android:label="@string/app_name">
            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER" />
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.slee_hdworak.huskybudget.ui.register.RegistrationActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.slee_hdworak.huskybudget.ui.main.MainActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.slee_hdworak.huskybudget.ui.edit.EditActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.slee_hdworak.huskybudget.ui.transaction.ModifyTransactionActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.slee_hdworak.huskybudget.ui.transaction.AddExpenseActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.slee_hdworak.huskybudget.ui.transaction.AddIncomeActivity"
            android:label="@string/app_name" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>